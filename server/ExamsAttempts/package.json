{
  "name": "examsattempts",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "NODE_ENV=development ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint .",
    "start": "NODE_ENV=production node -r ./config.js ./dist",
    "test": "NODE_ENV=test vitest",
    "type-check": "tsc --noEmit",
    "simulate:student": "ts-node tests/simulateStudent.ts",
    "simulate:teacher": "ts-node tests/simulateTeacher.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.6",
    "jet-logger": "^2.2.3",
    "jet-paths": "^2.0.1",
    "jet-validators": "^2.2.5",
    "jsonfile": "^6.2.0",
    "jsonwebtoken": "^9.0.3",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
